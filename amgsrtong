//Starting of class ArmstrongDemo
class ArmstrongDemo{ 

  //Starting of main
  public static void main(String[] args)  { 
 
    int c=0,a,temp; 

    //It is the number to check armstrong   
    int n=153;

    //Logic to identify a number Armstrong or not
    temp=n;  
    while(n>0)  
    {  
    a=n%10;  
    n=n/10;  
    c=c+(a*a*a);  
    }  
    if(temp==c)  
    System.out.println("Armstrong number");   
    else  
    System.out.println("Not armstrong number"); 

   //End of main  
   }  

//End of class
}  
