package Assignment8;
import java.util.Scanner;
public class IntegersWithOutZero {
	  public static void main(String[] args) {    
	    System.out.println("Please enter value of N: ");
	    Scanner sc = new Scanner(System.in);
	    int n = sc.nextInt();
	    //int largest = Integer.MIN_VALUE;
	    int smallest = Integer.MAX_VALUE;
	    System.out.printf("Please enter %d numbers %n", n);
	    for (int i = 0; i < n; i++) {
	      int num = sc.nextInt();
	      //if (current > largest) {
	        //largest = current;
	      //} 
	      if(num ==0)
	      {
	    	  System.out.println("Enter except 0");
	      }
	      else if(num < smallest) {
	        smallest = num;
	      }
	    }
	   // System.out.println("largest of N number is : " + largest);
	    System.out.println("smallest of N number is : " + smallest);
	  }
}
